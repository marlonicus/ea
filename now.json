{
  "type": "docker",
  "features": {
    "cloud": "v2"
  },
  "env": {
    "API_HOST": "@api-host",
    "AWS_COGNITO_APP_CLIENT_ID": "@aws-cognito-app-client-id",
    "AWS_COGNITO_POOL_ID": "@aws-cognito-pool-id",
    "AWS_COGNITO_REGION": "@aws-cognito-region",
    "DB_USER": "@db-user",
    "DB_PASS": "@db-pass",
    "DB_HOST": "@db-host",
    "DB_NAME": "@db-name",
    "EA_AWS_ACCESS_KEY_ID": "@ea-aws-key-id",
    "EA_AWS_ACCESS_SECRET": "@ea-aws-access-secret"
  }
}
